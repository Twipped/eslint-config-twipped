{
  "name": "@twipped/eslint-config",
  "version": "7.5.2",
  "description": "Personal eslint config for my own projects",
  "main": "node.js",
  "exports": {
    "./node-cjs": "./configs/node-cjs.js",
    "./node-esm": "./configs/node-esm.js",
    "./node-rollup": "./configs/node-rollup.js",
    "./browser": "./configs/browser.js",
    "./react": "./configs/react.js",
    "./react-native": "./configs/react-native.js",
    "./jsdoc": "./rules/jsdoc.js",
    "./jest": "./configs/jest.js"
  },
  "scripts": {
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "echo": "node echo.js",
    "release": "semantic-release",
    "publish:gar": "npm publish --registry https://npm.pkg.github.com"
  },
  "keywords": [],
  "author": "Jocelyn Badgley <joc@twipped.com> (http://twipped.com/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/Twipped/eslint-config-twipped.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "files": [
    "configs",
    "rules"
  ],
  "dependencies": {
    "@babel/core": "~7.18.5",
    "@babel/eslint-parser": "~7.18.2",
    "@babel/eslint-plugin": "^7.17.7",
    "@babel/preset-react": "~7.17.12",
    "confusing-browser-globals": "^1.0.11",
    "eslint-import-resolver-enhanced-resolve": "^1.0.5",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.7.0",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jsdoc": "~39.3.3",
    "eslint-plugin-jsx-a11y": ">=6.5.0",
    "eslint-plugin-n": ">= 15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "~7.30.0",
    "eslint-plugin-react-hooks": "~4.6.0",
    "eslint-plugin-react-native": "~4.0.0",
    "eslint-plugin-testing-library": "^5.6.0"
  },
  "peerDependencies": {
    "@babel/preset-env": ">=7",
    "eslint": ">=6"
  },
  "devDependencies": {
    "eslint": "latest",
    "sort-json": "^2.0.1",
    "tap": "~16.3.0"
  },
  "eslintConfig": {
    "extends": "./configs/node-cjs",
    "rules": {
      "n/exports-style": "off"
    },
    "ignorePatterns": [
      "**/__fixtures__/*"
    ]
  }
}
