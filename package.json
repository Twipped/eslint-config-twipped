{
  "name": "eslint-config-twipped",
  "version": "7.0.0",
  "description": "",
  "main": "node.js",
  "exports": {
    "./node-cjs": "./configs/node-cjs.js",
    "./node-esm": "./configs/node-esm.js",
    "./node-rollup": "./configs/node-rollup.js",
    "./browser": "./configs/browser.js",
    "./react": "./configs/react.js",
    "./react-native": "./configs/react-native.js",
    "./jsdoc": "./rules/jsdoc.js"
  },
  "scripts": {
    "lint": "eslint --fix .",
    "echo": "node echo.js",
    "release": "semantic-release"
  },
  "keywords": [],
  "author": "Jocelyn Badgley <joc@twipped.com> (http://twipped.com/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/Twipped/eslint-config-twipped.git"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "files": [
    "configs",
    "rules"
  ],
  "dependencies": {
    "@babel/core": "^7.17.9",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/eslint-plugin": "^7.17.7",
    "@babel/preset-react": ">=7",
    "confusing-browser-globals": "^1.0.11",
    "eslint-import-resolver-enhanced-resolve": "^1.0.5",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.1.0",
    "eslint-plugin-jsx-a11y": ">=6.5.0",
    "eslint-plugin-node": ">= 10.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-react-native": "~4.0.0"
  },
  "peerDependencies": {
    "@babel/preset-env": ">=7",
    "eslint": ">=6"
  },
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.4",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "conventional-changelog-conventionalcommits": "^4.6.3",
    "eslint": "latest",
    "semantic-release": "^19.0.2",
    "sort-json": "^2.0.1",
    "tap": "^16.0.1"
  },
  "eslintConfig": {
    "extends": "./configs/node-cjs",
    "rules": {
      "node/exports-style": "off"
    },
    "ignorePatterns": ["**/__fixtures__/*"]
  },
  "release": {
    "preset": "conventionalcommits",
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json"
          ]
        }
      ],
      "@semantic-release/github"
    ]
  }
}
