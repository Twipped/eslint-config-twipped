{
  "name": "@twipped/eslint-config",
  "version": "8.0.0",
  "description": "Personal eslint config for my own projects",
  "main": "node.js",
  "exports": {
    "./*": "./configs/*.js"
  },
  "scripts": {
    "test": "node test.js",
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "echo": "rimraf echo && node echo.js",
    "release": "semantic-release",
    "publish:gar": "npm publish --registry https://npm.pkg.github.com"
  },
  "keywords": [],
  "author": "Jocelyn Badgley <joc@twipped.com> (http://twipped.com/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/Twipped/eslint-config-twipped.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "files": [
    "configs",
    "rules"
  ],
  "dependencies": {
    "@babel/core": "^7.23.7",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/eslint-plugin": "^7.23.5",
    "@babel/preset-env": ">=7",
    "@babel/preset-react": "^7.23.3",
    "@testing-library/dom": ">=9.3.3",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^7.2.0",
    "confusing-browser-globals": "^1.0.11",
    "eslint-import-resolver-enhanced-resolve": "^1.0.5",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.1",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-jsdoc": "^48.0.2",
    "eslint-plugin-jsx-a11y": ">=6.8.0",
    "eslint-plugin-n": ">=16.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "eslint-plugin-testing-library": "^6.2.0"
  },
  "peerDependencies": {
    "@babel/preset-env": ">=7",
    "eslint": ">=6"
  },
  "devDependencies": {
    "eslint": "latest",
    "glob": "^10.3.10",
    "rimraf": "^5.0.5",
    "sort-json": "^2.0.1"
  },
  "eslintConfig": {
    "extends": "./configs/node-cjs",
    "rules": {
      "n/exports-style": "off"
    },
    "ignorePatterns": [
      "**/__fixtures__/*"
    ]
  }
}
